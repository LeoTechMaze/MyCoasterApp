{
  "name": "MyCoasterApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "lint": "eslint .",
    "prepare": "husky",
    "start": "react-native start",
    "test": "jest",
    "bump-version": "node ./scripts/bumpVersion.js"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.82.1",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.6.0",
    "react-native-safe-area-context": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.6",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
